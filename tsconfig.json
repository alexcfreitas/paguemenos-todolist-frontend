{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": [
      "dom",
      "es6"
    ],
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ]
}